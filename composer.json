{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=5.6.4",
        "laravel/framework": "5.4.*",
        "laravelcollective/html": "^5.3.0",
        "mews/purifier": "^2.0",
        "intervention/image": "^2.3",
        "barryvdh/laravel-debugbar": "^2.3",
        "cviebrock/eloquent-sluggable": ">=3.0.0-alpha",
        "doctrine/dbal": "2.5.0",
        "laracasts/generators": "~1.1",
        "laracasts/flash": "~1.3",
        "barryvdh/laravel-ide-helper": "~2.0",
        "creativeorange/gravatar": "~1.0",
        "anhskohbo/no-captcha": "^2.2",
        "ipunkt/laravel-analytics": "~1.0",
        "yajra/laravel-disqus": "^0.1.2",
        "unisharp/laravel-filemanager": "^1.7",
        "suin/php-rss-writer": "^1.5",
        "dflydev/apache-mime-types": "^1.0",
        "guzzlehttp/guzzle": "^6.2",
        "raiym/instagram-php-scraper": "^0.4.5",
        "barryvdh/laravel-snappy": "^0.3.1",
        "h4cc/wkhtmltopdf-amd64": "0.12.x",
        "h4cc/wkhtmltoimage-amd64": "0.12.x",
        "filp/whoops": "1.*",
        "artesaos/seotools": "^0.10.0",
        "anlutro/l4-settings": "^0.5.0",
        "laravel/tinker": "^1.0"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~5.7",
        "symfony/css-selector": "3.1.*",
        "symfony/dom-crawler": "3.1.*"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "files": [
          "app/Http/Helpers/instagram.php",
          "app/Support/helpers.php"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ]
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall",
            "php artisan optimize"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "php artisan optimize"
        ]
    },
    "config": {
        "preferred-install": "dist"
    }
}
